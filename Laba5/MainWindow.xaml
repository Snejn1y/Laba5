<Window x:Class="Laba5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laba5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="list" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,213,0"/>
        <TextBox x:Name="Start" HorizontalAlignment="Left" Margin="658,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="End" HorizontalAlignment="Left" Margin="658,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Crock" HorizontalAlignment="Left" Margin="658,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Start =" HorizontalAlignment="Left" Margin="607,20,0,0" VerticalAlignment="Top"/>
        <Label Content="End =" HorizontalAlignment="Left" Margin="611,58,0,0" VerticalAlignment="Top"/>
        <Label Content="Step =" HorizontalAlignment="Left" Margin="607,98,0,0" VerticalAlignment="Top"/>
        <Button Content="Calculate" HorizontalAlignment="Left" Margin="662,141,0,0" VerticalAlignment="Top" Height="51" Width="112" Click="Button_Click"/>

    </Grid>
</Window>
